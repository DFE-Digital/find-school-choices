  },
  "Views": [
    {
      "ViewId": "GDS_Breadcrumbs",
      "ViewContent": {
        "Type": "DfE.FindSchoolChoices.Web.Views.Shared.GDS_Breadcrumbs.GDS_BreadcrumbsContent, DfE.FindSchoolChoices.Web",
        "Links": [
          {
            "Text": "Home",
            "Href": "#"
          },
          {
            "Text": "Link 2",
            "Href": "#"
          },
          {
            "Text": "Link 3",
            "Href": "#"
          }
        ]
      }
    },
    {
      "ViewId": "MOJ_SideNavigation",
      "ViewContent": {
        "Type": "DfE.FindSchoolChoices.Web.Views.Shared.MOJ_SideNavigation.MOJ_SideNavigationContent, DfE.FindSchoolChoices.Web",
        "Links": [
          {
            "Text": "About the school",
            "Href": "#"
          },
          {
            "Text": "Admissions",
            "Href": "#"
          },
          {
            "Text": "Curriculum and extra curricular activities",
            "Href": "#"
          },
          {
            "Text": "Attendance",
            "Href": "#"
          },
          {
            "Text": "Academic performance",
            "Href": "#"
          },
          {
            "Text": "Destinations",
            "Href": "#"
          }
        ]
      }
    },
    {
      "ViewId": "GDS_SummaryList",
      "ViewModel": {
        "Type": "DfE.FindSchoolChoices.Web.Views.Shared.GDS_SummaryList.GDS_SummaryListModel, DfE.FindSchoolChoices.Web",
        "Heading": "About the school",
        "Data": [
          {
            "Key": "Name",
            "Value": "@establishments.EstablishmentName@"
          },
          {
            "Key": "Academy trust",
            "Value": "@establishments.['Trusts (name)']@ \r\n (updated in 2024)"
          },
          {
            "Key": "Website",
            "Value": "@establishments.SchoolWebsite@"
          },
          {
            "Key": "TelephoneNum",
            "Value": "@establishments.TelephoneNum@"
          }
        ]
      }
    },
    {
      "ViewId": "GDS_SummaryList",
      "ViewModel": {
        "Type": "DfE.FindSchoolChoices.Web.Views.Shared.GDS_SummaryList.GDS_SummaryListModel, DfE.FindSchoolChoices.Web",
        "Heading": "Location",
        "Data": [
          {
            "Key": "Address",
            "Value": "@establishments.Street@, @establishments.Locality@, @establishments.Town@, @establishments.Postcode@"
          },
          {
            "Key": "Local authority",
            "Value": "@establishments.LA_name@"
          },
          {
            "Key": "Your distance from this school",
            "Value": "1.2 miles"
          }
        ]
      }
    },
    {
      "ViewId": "GDS_Details",
      "ViewContent": {
        "Type": "DfE.FindSchoolChoices.Web.Views.Shared.GDS_Details.GDS_DetailsContent, DfE.FindSchoolChoices.Web",
        "Summary": "View this information on a map",
        "Description": ""
      }
    }
  ]
}